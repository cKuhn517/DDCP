<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:background="#FFFFFF"
        android:stretchColumns="0,1,2,3,4"
        android:id="@+id/tableLayout_cs2_abilities">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#000000"
            android:paddingBottom="3dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Ability"
                android:padding="5dp"
                android:id="@+id/textView_cs2_ability"
                android:layout_column="0"
                android:background="#FFFFFF"
                android:layout_margin="1dp"
                android:textStyle="bold"
                android:gravity="center_vertical|center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Score"
                android:padding="5dp"
                android:id="@+id/textView_cs2_score"
                android:layout_column="1"
                android:background="#FFFFFF"
                android:layout_margin="1dp"
                android:textStyle="bold"
                android:gravity="center_vertical|center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Mod"
                android:padding="5dp"
                android:id="@+id/textView_cs2_modifier"
                android:layout_column="2"
                android:background="#FFFFFF"
                android:layout_margin="1dp"
                android:textStyle="bold"
                android:gravity="center_vertical|center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Temp\nScore"
                android:padding="5dp"
                android:id="@+id/textView_cs2_tempscore"
                android:layout_column="3"
                android:background="#FFFFFF"
                android:layout_margin="1dp"
                android:textStyle="bold"
                android:gravity="center_vertical|center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Temp\nMod"
                android:padding="5dp"
                android:id="@+id/textView_cs2_tempmod"
                android:layout_column="4"
                android:background="#FFFFFF"
                android:layout_margin="1dp"
                android:textStyle="bold"
                android:gravity="center_vertical|center_horizontal" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#000000">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="STR"
                android:padding="5dp"
                android:id="@+id/textView_cs2_str"
                android:layout_column="0"
                android:background="#bfbfbf"
                android:layout_margin="1dp"
                android:gravity="center_vertical|center_horizontal" />

            <EditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="5dp"
                android:id="@+id/editText_cs2_strscore"
                android:inputType="number"
                android:layout_column="1"
                android:background="#FFFFFF"
                android:layout_margin="1dp"
                android:text="10"
                android:gravity="center_vertical|center_horizontal"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:maxLength="2"
                android:numeric="integer" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="0"
                android:padding="5dp"
                android:id="@+id/textView_cs2_strmod"
                android:layout_column="2"
                android:background="#bfbfbf"
                android:layout_margin="1dp"
                android:gravity="center_vertical|center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="0"
                android:padding="5dp"
                android:id="@+id/textView_cs2_strtempscore"
                android:layout_column="3"
                android:background="#bfbfbf"
                android:layout_margin="1dp"
                android:gravity="center_vertical|center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="0"
                android:padding="5dp"
                android:id="@+id/textView_cs2_strtempmod"
                android:layout_column="4"
                android:background="#bfbfbf"
                android:layout_margin="1dp"
                android:gravity="center_vertical|center_horizontal" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#000000">

            <TextView
