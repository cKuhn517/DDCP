                android:text="0"
                android:padding="5dp"
                android:id="@+id/textView_cs2_inttempscore"
                android:layout_column="3"
                android:background="#bfbfbf"
                android:layout_margin="1dp"
                android:gravity="center_vertical|center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="0"
                android:padding="5dp"
                android:id="@+id/textView_cs2_inttempmod"
                android:layout_column="4"
                android:background="#bfbfbf"
                android:layout_margin="1dp"
                android:gravity="center_vertical|center_horizontal" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#000000">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="WIS"
                android:padding="5dp"
                android:id="@+id/textView_cs2_wis"
                android:layout_column="0"
                android:background="#bfbfbf"
                android:layout_margin="1dp"
                android:gravity="center_vertical|center_horizontal" />

            <EditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="5dp"
                android:id="@+id/editText_cs2_wisscore"
                android:inputType="number"
                android:layout_column="1"
                android:background="#FFFFFF"
                android:layout_margin="1dp"
                android:text="10"
                android:gravity="center_vertical|center_horizontal"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:maxLength="2"
                android:numeric="integer" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="0"
                android:padding="5dp"
                android:id="@+id/textView_cs2_wismod"
                android:layout_column="2"
                android:background="#bfbfbf"
                android:layout_margin="1dp"
                android:gravity="center_vertical|center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="0"
                android:padding="5dp"
                android:id="@+id/textView_cs2_wistempscore"
                android:layout_column="3"
                android:background="#bfbfbf"
                android:layout_margin="1dp"
                android:gravity="center_vertical|center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="0"
                android:padding="5dp"
                android:id="@+id/textView_cs2_wistempmod"
                android:layout_column="4"
                android:background="#bfbfbf"
                android:layout_margin="1dp"
                android:gravity="center_vertical|center_horizontal" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#000000">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="CHA"
                android:padding="5dp"
                android:id="@+id/textView_cs2_cha"
                android:layout_column="0"
                android:background="#bfbfbf"
                android:layout_margin="1dp"
